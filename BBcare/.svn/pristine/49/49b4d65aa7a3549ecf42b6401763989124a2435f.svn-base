package admin.vo;

public class Report {
	private int ReportID;
	private int ReportedContentID;
	private String ContentType;
	private String ReporterUserID;
	private String ReportedUserID;
	private String ReportDate;
	private String ReportReason;
	private String Status;
	public Report() {
		// TODO Auto-generated constructor stub
	}

	public String getReportedUserID() {
		return ReportedUserID;
	}

	public void setReportedUserID(String reportedUserID) {
		ReportedUserID = reportedUserID;
	}

	public Report(int reportID, int reportedContentID, String contentType, String reporterUserID, String reportedUserID,
			String reportDate, String reportReason, String status) {
		ReportID = reportID;
		ReportedContentID = reportedContentID;
		ContentType = contentType;
		ReporterUserID = reporterUserID;
		ReportedUserID = reportedUserID;
		ReportDate = reportDate;
		ReportReason = reportReason;
		Status = status;
	}

	public int getReportID() {
		return ReportID;
	}
	public void setReportID(int reportID) {
		ReportID = reportID;
	}
	public int getReportedContentID() {
		return ReportedContentID;
	}
	public void setReportedContentID(int reportedContentID) {
		ReportedContentID = reportedContentID;
	}
	public String getContentType() {
		return ContentType;
	}
	public void setContentType(String contentType) {
		ContentType = contentType;
	}
	public String getReporterUserID() {
		return ReporterUserID;
	}
	public void setReporterUserID(String reporterUserID) {
		ReporterUserID = reporterUserID;
	}
	public String getReportDate() {
		return ReportDate;
	}
	public void setReportDate(String reportDate) {
		ReportDate = reportDate;
	}
	public String getReportReason() {
		return ReportReason;
	}
	public void setReportReason(String reportReason) {
		ReportReason = reportReason;
	}
	public String getStatus() {
		return Status;
	}
	public void setStatus(String status) {
		Status = status;
	}
}
