package community.vo;

import java.sql.Date;

public class Board {
	
	private int no;	//번호
	private String id;	//id
	private String name;	//닉네임
	private Date time;		//작성시간
	private String title;	//제목 
	private String img;		//이미지경로
	private String content;	//내용
	private int likes;		//좋아요
	private int click;		//좋아요
	
	public Board() {}

	public Board(int no, String id, String name, Date time, 
			String title, String img, String content, int likes,
			int click) {
		super();
		this.no = no;
		this.id = id;
		this.name = name;
		this.time = time;
		this.title = title;
		this.img = img;
		this.content = content;
		this.likes = likes;
		this.click = click;
	}


	public int getNo() {
		return no;
	}


	public void setNo(int no) {
		this.no = no;
	}


	public String getId() {
		return id;
	}


	public void setId(String id) {
		this.id = id;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public Date getTime() {
		return time;
	}


	public void setTime(Date time) {
		this.time = time;
	}


	public String getTitle() {
		return title;
	}


	public void setTitle(String title) {
		this.title = title;
	}


	public String getImg() {
		return img;
	}


	public void setImg(String img) {
		this.img = img;
	}


	public String getContent() {
		return content;
	}


	public void setContent(String content) {
		this.content = content;
	}


	public int getLikes() {
		return likes;
	}


	public void setLikes(int likes) {
		this.likes = likes;
	}


	public int getClick() {
		return click;
	}


	public void setClick(int click) {
		this.click = click;
	}

	
	
	
	
}
