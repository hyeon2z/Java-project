package backendweb.z01_vo;
// 
public class BSPlayer {
	private String player;
	private double hseat;
	private double hcnt;
	public BSPlayer() {
		// TODO Auto-generated constructor stub
	}
	public BSPlayer(String player, double hseat, double hcnt) {
		this.player = player;
		this.hseat = hseat;
		this.hcnt = hcnt;
	}
	public double getHitRatio() {
		return hseat==0?0:hcnt/hseat;
	}	
	public String getPlayer() {
		return player;
	}
	public void setPlayer(String player) {
		this.player = player;
	}
	public double getHseat() {
		return hseat;
	}
	public void setHseat(double hseat) {
		this.hseat = hseat;
	}
	public double getHcnt() {
		return hcnt;
	}
	public void setHcnt(double hcnt) {
		this.hcnt = hcnt;
	}

	
}
