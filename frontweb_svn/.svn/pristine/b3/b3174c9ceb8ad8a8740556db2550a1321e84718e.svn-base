function multiply(start, end) {
  if (start < 1 || end < 1 || start > 9 || end > 9) {
    ("1~9사이의 수로 시작하여야 합니다.");
    return;
  }

  for (let i = start; i <= end; i++) {
    console.log(`--- ${i}단 ---`);
    for (let j = 1; j <= 9; j++) {
      console.log(`${i} x ${j} = ${i * j}`);
    }
  }
}
multiply(1,3); 
