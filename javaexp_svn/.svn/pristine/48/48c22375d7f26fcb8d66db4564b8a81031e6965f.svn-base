package javaexp.z02_homework.a06_psj.vo;

public class Coffee {
	String brand;
	int price;
	String origin;
	public Coffee(String brand) {
		this.brand = brand;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public void setOrigin(String origin) {
		this.origin = origin;
	}
	public void coffeeInfo() {
		System.out.println("\n커피 종류: "+brand);
		System.out.println("가격: "+price);
		System.out.println("원산지: "+origin);
	}
}
