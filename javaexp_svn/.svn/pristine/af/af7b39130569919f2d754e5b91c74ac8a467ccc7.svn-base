package javaexp.z02_homework.a06_psj.vo;

public class Student {
	private int StuNum;
	private String name;
	private int score;
	public Student(int stuNum, String name) {
		StuNum = stuNum;
		this.name = name;
	}
	public int getScore() {
		return score;
	}
	public void setScore(int score) {
		this.score = score;
	}
	public void StuInfo() {
		System.out.println("\n이름: "+name);
		System.out.println("학번: "+StuNum);
		System.out.println("성적: "+score);
	}
}
