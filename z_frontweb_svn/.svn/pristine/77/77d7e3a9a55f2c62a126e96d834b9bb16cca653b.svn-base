<!-- [1단계:개념] 5. 선택자로 $("선택자1 선택자2")와 $("선택자1>선택자")의 차이점을 예제로 기술하세요.-->
<script type="text/javascript" src="jquery-3.1.1.js"></script>
<script type="text/javascript">
    $(document).ready(function(){   
       $("h2").css({color:"pink"}) //상위 하위 관계
       
    });

</script>
<body>
    <h2>하이</h2>
    <h2>안녕</h2>
    <h2>굿</h2>
</body> 


