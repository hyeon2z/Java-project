function gugu(start, end) {
    start = Number(prompt("시작단",0))
    end = Number(prompt("끝단",0))
    for(start; start <= end; start ++) {
        for(i = 1; i <= 9; i ++) {
            console.log(`${start} x ${i} = ${start * i}`)
        }
    }
}
gugu()

