package javaexp.z02_homework.a03_ls.vo;

public class Stu {
	String name;
	int no;
	public Stu() {
		// TODO Auto-generated constructor stub
	}
	public Stu(String name, int no) {
		this.name = name;
		this.no = no;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getNo() {
		return no;
	}
	public void setNo(int no) {
		this.no = no;
	}
	

}
