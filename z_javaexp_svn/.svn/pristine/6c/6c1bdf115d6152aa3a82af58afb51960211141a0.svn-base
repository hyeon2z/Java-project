package javaexp.z02_homework.a20_kjw;

public class rectangle {

	
		
		public double width;
		public double height;
			public rectangle(double width, double height) {
				super();
				this.width = width;
				this.height = height;
			}
			
			public double getArea() {
				return width*height;
			}
			
			
			public double getWidth() {
				return width;
			}
		
			public double getHeight() {
				return height;
			}
}


	

	
