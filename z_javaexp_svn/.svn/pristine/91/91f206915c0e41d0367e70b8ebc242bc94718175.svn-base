package javaexp.z02_homework.a04_asy.vo;

public class Student {
//	- 필드: 학번, 이름, 성적
//    - 생성자: 학번과 이름 초기화
//    - 메서드: 성적 입력, 성적 조회, 정보 출력
	private int number;
	private String name;
	private int point;
	
	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPoint() {
		return point;
	}

	public void setPoint(int point) {
		this.point = point;
	}

	public Student(int number, String name, int point) {
		super();
		this.number = number;
		this.name = name;
		this.point = point;
	}
	
}
