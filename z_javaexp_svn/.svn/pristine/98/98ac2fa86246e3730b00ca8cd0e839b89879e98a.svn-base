package javaexp.z02_homework.a06_psj.vo;

public class Travel {
	String destination;
	int departureDay;
	int arrivalDay;
	public Travel(String destination, int departureDay, int arrivalDay) {
		this.destination = destination;
		this.departureDay = departureDay;
		this.arrivalDay = arrivalDay;
	}
	public void day() {
		System.out.println("\n여행일수: "+(arrivalDay-departureDay));
	}
	public void travelInfo() {
		System.out.println("\n목적지: "+destination);
		System.out.println("출발일: "+departureDay);
		System.out.println("도착일: "+arrivalDay);
	}
}
