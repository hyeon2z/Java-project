package javaexp.z02_homework.a11_mhi.vo;

public class Student2 {
	private String studntId;
	private String name;
	private double grade;

	public Student2(String studntId, String name, double grade) {
		this.studntId = studntId;
		this.name = name;
		this.grade = grade;
	}


	// 직접 입력
	public void showInf() {
		System.out.println("# Student객체의 정보 #");
		System.out.println("학번:" + studntId);
		System.out.println("이름:" + name);
		System.out.println("성적:" + grade);
	}

	public void setGrade(double grade) {
		this.grade = grade;
	}

}
