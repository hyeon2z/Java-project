package javaexp.z02_homework.a14_cms.vo;

public class Home {
	private String location; //집의 위치
	private String type; //집의 형태 (빌라나 아파트 등)
	private int size; //평수
	public Home() {
	}
	public Home(String location, String type, int size) {
		this.location = location;
		this.type = type;
		this.size = size;
	}
	public void showHomeInfo() {
		System.out.println("집주소: " + location);
		System.out.println("집의 형태: " + type);
		System.out.println("집 평수: " + size);
	}
	public String getLocation() {
		return location;
	}
	public void setLocation(String location) {
		this.location = location;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	
	
	
	

}
