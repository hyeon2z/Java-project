package javaexp.z02_homework.a14_cms.vo;

public class Student {
	private int stNo;
	private String name;
	private String grade;
	
	public Student(int stNo, String name){
		this.stNo = stNo;
		this.name = name;
	}
	public void inputGrade(String grade) {
		this.grade = grade;
	}
	public void getGrade() {
		System.out.println(name+"의 성적은: " + grade);
	}
	public void studentInfo() {
		System.out.println("학생 정보 출력");
		System.out.println("학생 이름: "+name );
		System.out.println("학생 번호: " + stNo);
		System.out.println("학생 성적" + grade);
		
		
		
	}
}
